<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('fotoclub') }}
        {% endblock %}
    </head>
    <body>
        <div class="container">
            <header class="row header-container">
            </header>


            <main class="mainContent">
                {% block body %}{% endblock %}
            </main>

            <footer class="footer">
                <div class="col-lg-12 navbar navbar-fotoclub">
                    <div class="container-fluid">
                        <ul class="nav navbar-nav">
                            <li><a class="footerLink" href="">Home</a></li>
                            <li><a class="footerLink" href="">Galerij</a></li>
                            <li><a class="footerLink" href="">Nieuws</a></li>
                            <li><a class="footerLink" href="">Agenda</a></li>
                            <li><a class="footerLink" href="">Contact</a></li>
                            <li><a class="footerLink" href="">Info</a></li>
                            <li><a class="footerLink" href="">Links</a></li>
                        </ul>
                    </div>
                </div>
            </footer>
        </div>
        {% block javascripts %}
            {{ encore_entry_script_tags('fotoclub') }}
        {% endblock %}
    </body>
</html>
